{
	"name": "npm-package-starter",
	"version": "0.0.0",
	"description": "npm-package-starter",
	"keywords": [
		"npm-package-starter"
	],
	"repository": "rocktimsaikia/npm-package-starter",
	"funding": "https://github.com/sponsors/rocktimsaikia",
	"license": "MIT",
	"author": {
		"name": "Rocktim Saikia",
		"email": "rocktimthedev@gmail.com",
		"url": "https://rocktimsaikia.dev"
	},
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"lint": "biome lint .",
		"lint:fix": "biome lint --write .",
		"format": "biome format --write .",
		"release": "np --no-publish",
		"test": "vitest"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "^22.14.1",
		"tsup": "^8.4.0",
		"tsx": "^4.19.3",
		"typescript": "^5.8.3",
		"vitest": "^3.1.1"
	},
	"packageManager": "pnpm@10.8.0",
	"engines": {
		"node": ">=18"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"format": [
			"cjs",
			"esm"
		],
		"minify": true,
		"clean": true,
		"dts": true
	}
}
